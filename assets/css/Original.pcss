    width: 10px;
    -webkit-backface-visibility: hidden;
    -webkit-transition: left;
    transition: left;
    -webkit-transition-delay: 0s;
    transition-delay: 0s;
    -webkit-transition-duration: 1s;
    transition-duration: 1s;
    -webkit-transition-timing-function: 1s;
